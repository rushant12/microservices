package com.broadridge.pdf2print.dataprep.models;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class FileCoordinate {
    private double x1Pos;
    private double y1Pos;
    private double x2Pos;
    private double y2Pos;
    private int iOrientation;
    private int pagenum;

    // Manually define the constructor for the case with 5 parameters
    public FileCoordinate(double x1Pos, double y1Pos, double x2Pos, double y2Pos, int iOrientation) {
        this.x1Pos = x1Pos;
        this.y1Pos = y1Pos;
        this.x2Pos = x2Pos;
        this.y2Pos = y2Pos;
        this.iOrientation = iOrientation;
    }
}
