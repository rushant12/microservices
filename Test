package com.broadridge.pdf2print.dataprep.models;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class FileCoordinate {
    private double x1Pos;
    private double y1Pos;
    private double x2Pos;
    private double y2Pos;
    private int iOrientation;
    private int pagenum;
    
    
	public FileCoordinate(double x1Pos, double y1Pos, double x2Pos, double y2Pos, int iOrientation, int pagenum) {
		super();
		this.x1Pos = x1Pos;
		this.y1Pos = y1Pos;
		this.x2Pos = x2Pos;
		this.y2Pos = y2Pos;
		this.iOrientation = iOrientation;
		this.pagenum = pagenum;
	}
	   
		public FileCoordinate(double x1Pos, double y1Pos, double x2Pos, double y2Pos, int iOrientation) {
			super();
			this.x1Pos = x1Pos;
			this.y1Pos = y1Pos;
			this.x2Pos = x2Pos;
			this.y2Pos = y2Pos;
			this.iOrientation = iOrientation;
		}
	public double getX1Pos() {
		return x1Pos;
	}
	public void setX1Pos(double x1Pos) {
		this.x1Pos = x1Pos;
	}
	public double getY1Pos() {
		return y1Pos;
	}
	public void setY1Pos(double y1Pos) {
		this.y1Pos = y1Pos;
	}
	public double getX2Pos() {
		return x2Pos;
	}
	public void setX2Pos(double x2Pos) {
		this.x2Pos = x2Pos;
	}
	public double getY2Pos() {
		return y2Pos;
	}
	public void setY2Pos(double y2Pos) {
		this.y2Pos = y2Pos;
	}
	public int getiOrientation() {
		return iOrientation;
	}
	public void setiOrientation(int iOrientation) {
		this.iOrientation = iOrientation;
	}
	public int getPagenum() {
		return pagenum;
	}
	public void setPagenum(int pagenum) {
		this.pagenum = pagenum;
	}
	@Override
	public String toString() {
		return "FileCoordinate [x1Pos=" + x1Pos + ", y1Pos=" + y1Pos + ", x2Pos=" + x2Pos + ", y2Pos=" + y2Pos
				+ ", iOrientation=" + iOrientation + ", pagenum=" + pagenum + "]";
	}
   
}
